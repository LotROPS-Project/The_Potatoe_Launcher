; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="Potatoe Launcher"
run/low_processor_mode=true
boot_splash/image="res://Resources/Potatoe_logo.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://Resources/Potatoe_logo.png"
config/macos_native_icon="res://Resources/Potatoe_logo.png"
config/windows_native_icon="res://Resources/Potatoe_logo.png"
config/auto_accept_quit=false

[audio]

mix_rate=48000

[display]

window/dpi/allow_hidpi=true
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[gui]

theme/use_hidpi=true

[physics]

common/enable_pause_aware_picking=true
3d/active_soft_world=false
2d/thread_model=2

[rendering]

quality/driver/fallback_to_gles2=true
threads/thread_model=2
misc/lossless_compression/force_png=true
quality/shadows/filter_mode=0
quality/filters/anisotropic_filter_level=16
portals/gameplay/use_signals=false
quality/subsurface_scattering/quality=0
quality/filters/msaa=1
environment/default_environment="res://default_env.tres"
